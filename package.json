{
  "name": "teamwork-backend",
  "version": "1.0.0",
  "description": "DevC TrainingWithAndela - Capstone Project",
  "main": "server.js",
  "engines": {
    "node": "13.x"
  },
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "nodemon --exec babel-node buildScripts/server.js",
    "security:check": "npm audit",
    "lint": "esw src buildScripts config.js --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "nyc --reporter=html --reporter=text mocha buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/francisetim/teamwork-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/francisetim/teamwork-backend/issues"
  },
  "homepage": "https://github.com/francisetim/teamwork-backend#readme",
  "dependencies": {
    "babel-register": "^6.26.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint-watch": "^6.0.1",
    "express": "^4.17.1",
    "pg": "^7.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jsdom": "^15.2.1",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1"
  }
}
